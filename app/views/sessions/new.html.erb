<h1> Sign In </h1>

<form method="post" action="<%= session_url %>">

  <input
    type="hidden"
    name="authenticity_token"
    value="<%= form_authenticity_token %>">

  <label for="username"> Username : </label>
  <input
    id="username"
    type="text"
    name="user[username]">
  <br>

  <label for="password"> Password : </label>
  <input
    id="password"
    type="password"
    name="user[password]">
  <br>

  <button
    type="submit">
    Sign In!
  </button>
</form>
